    RowBox[{"+", "\[Kappa]"}], " ", 
    SubscriptBox["T", "ab"]}]}]],ExpressionUUID->
  "f3356504-ab26-4716-9a81-84d632957fd9"],
 " (",
 Cell[BoxData[
  RowBox[{"\[Kappa]", "=", 
   RowBox[{"8", " ", "\[Pi]", " ", 
    RowBox[{"G", "/", 
     SuperscriptBox["c", "4"]}]}]}]],ExpressionUUID->
  "11a8d0e0-2612-4f03-9285-9255e57eda76"],
 ")."
}], "Text",
 FontSize->16,ExpressionUUID->"8d6595cf-1c7f-42a9-8970-7ce740c9b62f"]
